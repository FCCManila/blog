{
  "name": "fccmanila-blog",
  "version": "0.1.0-alpha",
  "description": "Free Code Camp Manila Official Blog",
  "main": "n/a",
  "scripts": {
    "start": "npm run dev",
    "dev": "gatsby develop",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public . || exit 0",
    "lint:md": "markdownlint *.md docs/*.md pages/*.md pages/**/*.md || exit 0",
    "test": "npm run lint",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-links && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FCCManila/blog.git"
  },
  "dependencies": {
    "gatsby": "^1.9.250",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-feed": "^1.3.20",
    "gatsby-plugin-google-analytics": "^1.0.29",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "gatsby-plugin-sharp": "^1.6.42",
    "gatsby-plugin-typography": "^1.7.18",
    "gatsby-remark-copy-linked-files": "^1.5.31",
    "gatsby-remark-images": "^1.5.61",
    "gatsby-remark-prismjs": "^1.2.24",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-remark-smartypants": "^1.4.12",
    "gatsby-source-filesystem": "^1.5.31",
    "gatsby-transformer-remark": "^1.7.40",
    "gatsby-transformer-sharp": "^1.6.22",
    "lodash": "^4.17.5",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43",
    "typography-theme-wordpress-2016": "^0.15.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "gh-pages": "^1.1.0",
    "markdownlint": "^0.3.1",
    "markdownlint-cli": "^0.2.0",
    "prettier": "^1.12.0"
  },
  "keywords": [
    "code",
    "coding",
    "coder",
    "codes",
    "coders",
    "program",
    "programming",
    "programs",
    "programmer",
    "programmers",
    "philippines",
    "pilipinas",
    "filipino",
    "pilipino",
    "manila",
    "community",
    "learning",
    "education",
    "self-study",
    "javascript",
    "frontend",
    "backend"
  ],
  "author": "Free Code Camp Manila (https://www.facebook.com/groups/free.code.camp.manila)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FCCManila/blog/issues"
  },
  "homepage": "https://github.com/FCCManila/blog#readme"
}
